{
    "buildinfo": [
    {
        "prerun": [
            "curl -sSL -o ~/.cache/deb2appimage/debs/gprename.deb 'http://mirrors.kernel.org/ubuntu/pool/universe/g/gprename/gprename_20210415-1_all.deb'"
        ],
        "name": "gprename",
        "version": "20210415",
        "deps": "libglib-perl,libintl-perl,liblocale-gettext-perl,libpango-perl,perl,libgtk3-perl",
        "repoarch": "amd64,all,amd64,amd64,amd64,all",
        "distrorepo": "Ubuntu",
        "repoversion": "jammy",
        "binarypath": "/usr/bin/gprename",
        "desktoppath": "/usr/share/applications/gprename.desktop",
        "iconpath": null,
        "usewrapper": "true",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "true",
        "setlibpath": "true",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "true",
        "setgsettingsschemadir": "true",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/gprename.wrapper"
    }
    ],
    "authors": [
    {
        "type": "Author",
        "author": "jwilm",
        "url": "https://github.com/jwilm/alacritty"
    },
    {
        "type": "AppImage Maintainer",
        "author": "simonizor",
        "url": "http://www.simonizor.net"
    }
    ]
} 
